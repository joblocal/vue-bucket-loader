{
  "name": "@joblocal/vue-bucket-loader",
  "version": "1.0.3",
  "main": "./dist/main.js",
  "license": "MIT",
  "author": "Joblocal GmbH",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "vue",
    "AWS S3",
    "file upload",
    "SSR"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --hot",
    "prebuild": "rm -rf dist && npm run validate",
    "prepublishOnly": "npm run build",
    "build": "webpack -p",
    "test": "jest src",
    "test:watch": "npm test -- --watch --onlyChanged",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint . --ignore-path .gitignore",
    "validate": "npm-run-all --parallel lint test",
    "release": "npm publish --access public"
  },
  "peerDependencies": {
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^22.3.0",
    "npm-run-all": "^4.1.2",
    "pre-push": "^0.1.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "vue": "^2.5.13",
    "vue-jest": "^2.2.1",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "pre-push": {
    "run": "validate",
    "silent": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/**/*.(test|spec).js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleNameMapper": {
      "^src/(.*/[A-Z].+)$": "<rootDir>/src/$1.vue"
    },
    "transform": {
      "^.*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "description": "This Project provides a SSR compatible vue component which can handle file upload / deleting directly to / from a AWS S3 Bucket.",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joblocal/vue-bucket-loader.git"
  },
  "bugs": {
    "url": "https://github.com/joblocal/vue-bucket-loader/issues"
  },
  "homepage": "https://github.com/joblocal/vue-bucket-loader#readme"
}
